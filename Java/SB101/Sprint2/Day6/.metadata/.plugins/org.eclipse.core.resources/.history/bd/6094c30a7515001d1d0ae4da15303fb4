package Question2;

public class Question1 {
	
	public static void main(String[] args) {
		
		ThreadA a = new  ThreadA();
		
		Thread thread = new Thread(a);
		thread.start();
		
		
		
		
		
	}
	

}

class ThreadA implements Runnable {
   
	int prouct;

	
	@Override
	public void run() {
		for(int a = 1 ; a<=10 ;a++) {
			prouct = prouct*a;
		}
	}
	
}
