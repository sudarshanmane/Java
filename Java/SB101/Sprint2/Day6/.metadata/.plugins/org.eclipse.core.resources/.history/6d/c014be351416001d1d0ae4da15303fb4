package Question2;

public class ThreadPool {
	
    public static void main(String[] args) {
		
    	Thread thread = new Thread(new Common("pawan"));
    	thread.start();
    	
	}

}

class Common implements Runnable{
	
	String name;
	
	public Common(String name) {	
		this.name= name;
		
	}



	@Override
	public void run() {
		
		synchronized (common) {
			
			System.out.println(name);
			
		}
		

		
	}
	
	
}
