package com.masai;

public enum CityNew {
	
	PUNE("PU"){
		
		@Override
		public void checkName(String city) {
			
			System.out.println("Pune " + " short form is : " + shortN);
			
		}
		
		
	},MUMBAI("MUM"),OMERGA("OME"),LATUR("LTR"),OSMANABAD("OSMA");
	
	String shortN ;
	
	private CityNew(String shortName) {
		
		this.shortN = shortName;

	}
	
	public void checkName(String city) {
		System.out.println(city + " Short form is : " + this.shortN);
	}
	
	


}
