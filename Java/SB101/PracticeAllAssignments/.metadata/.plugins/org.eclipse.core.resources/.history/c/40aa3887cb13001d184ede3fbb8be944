
public class Student {
	
	int RollNo;
	String name; 
	int marks;
	
	public Student(int rollNo, String name, int marks) {
		super();
		RollNo = rollNo;
		Name = name;
		this.marks = marks;
	}

	@Override
	public String toString() {
		return "Student [RollNo=" + RollNo + ", Name=" + Name + ", marks=" + marks + "]";
	}
	
	
	
	  
}
