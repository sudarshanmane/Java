
public class Mobile {

	String[] outdatedModels = {"note4","note5","note6","note7"};
	  
	
	
     void searchOutdatedModel(String nameofCompany,int...num){
    	 
    	System.out.println(nameofCompany);
    	
    	int n = 0;
    	
