package PassingLambda;

public class RLamda {

	public static void main(String[] args) {
		
		returningLambda().getSum(10,1000);
	}
	
	public static MyInterface2 returningLambda() {
		
		return (a,b) -> System.out.println( a + b );
		
	}

}

@FunctionalInterface

interface MyInterface2 {
	
	abstract void getSum(int a ,int b);
	
	
}
