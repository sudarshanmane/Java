package FilesIO;

import java.io.File;
import java.io.IOException;

public class _1FilesIO {

	public static void main(String[] args) {
		
		File file = new File("d://abx.txt"); 
		try {
			file.createNewFile();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println(file.exists());
		
	}
}
