package Question7;

import java.util.List;

public class Student {
	
	int RollNo;
	String Name;
	int marks;
	Student

	public Student(int rollNo, String name, int marks) {
		super();
		RollNo = rollNo;
		Name = name;
		this.marks = marks;
	}
	
	
	@Override
	public String toString() {
		return "Student [RollNo=" + RollNo + ", Name=" + Name + ", marks=" + marks + "]";
	}

}
