
public class Test  implements Intr{

	@Override
	public int[] display(int p) {
		int n = 1;
		int arrOf = 0;
		int[] arr = new int[n]; 
		
		for(int a =0;a<p;a++) {
			
			int count = 0;
			for(int k =1;k<=a;k++) {
				if(a%k == 0) {
					count++;
				}
			}
			if(count==2) {
				arr[0] = a;
				arrOf++;
				
				n++;
				System.out.println(a);
			}
		}
		
		return arr;
	}
	

}
