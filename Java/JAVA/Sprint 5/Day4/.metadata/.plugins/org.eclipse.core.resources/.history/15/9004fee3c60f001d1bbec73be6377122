
public class Students {

	int RollNo; 
	String Name;
	int marks;
	
	public Students(int rollNo, String name, int marks) {
		super();
		RollNo = rollNo;
		Name = name;
		this.marks = marks;
	}

	@Override
	public String toString() {
		return "Students [RollNo=" + RollNo + ", Name=" + Name + ", marks=" + marks + "]";
	}
	
	
	

}
