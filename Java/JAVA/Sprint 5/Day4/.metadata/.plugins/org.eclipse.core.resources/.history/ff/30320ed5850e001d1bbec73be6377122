
public class MyGenerics {
	
	public static void main(String[] args) {
		
		MyOwnGenerics<String> t = new MyOwnGenerics<>();
		t.add("one");
		t.add("two");
		System.out.println(t);
		
	}
	
	
	

}


class MyOwnGenerics<T> {
	
	T obj;
	
	void add(T object) {
	
		this.obj= object;
		
	}

	T get() {
		
		return obj;
	}
	
}